{
  "system": {
    "name": "Tesseract Pattern Processor",
    "version": "1.0.0",
    "description": "8D pattern analysis with quantum-cellular synchronization"
  },
  "processing": {
    "update_interval": 1.0,
    "history_size": 1000,
    "pattern_window": 100,
    "coherence_threshold": 0.8,
    "homeostasis_threshold": 0.7,
    "entropy_target": 0.5
  },
  "visualization": {
    "enabled": true,
    "port": 3000,
    "theme": "dark",
    "update_rate": 1000,
    "dimensions": {
      "labels": [
        "Market_Sentiment",
        "Price_Momentum",
        "Volume_Pressure",
        "Volatility_Index",
        "Temporal_Phase",
        "Liquidity_Depth",
        "Whale_Activity",
        "Technical_Signal"
      ],
      "colors": [
        "#3B82F6",
        "#10B981",
        "#F59E0B",
        "#EF4444",
        "#8B5CF6",
        "#EC4899",
        "#06B6D4",
        "#F97316"
      ]
    }
  },
  "quantum": {
    "coherence_window": 5,
    "correction_strength": 0.3,
    "stabilization_factor": 0.7
  },
  "cellular": {
    "homeostasis_window": 5,
    "regulation_strength": 0.2,
    "balance_factor": 0.8
  },
  "logging": {
    "enabled": true,
    "level": "INFO",
    "file": "logs/tesseract.log",
    "max_size": 10485760,
    "backup_count": 5
  },
  "monitoring": {
    "enabled": true,
    "metrics": {
      "coherence": true,
      "homeostasis": true,
      "stability": true,
      "entropy": true,
      "pattern_variance": true
    },
    "alerts": {
      "coherence_threshold": 0.7,
      "homeostasis_threshold": 0.6,
      "stability_threshold": 0.5
    }
  }
} 